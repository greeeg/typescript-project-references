{
  "compilerOptions": {
    "declaration": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "disableSourceOfProjectReferenceRedirect": true,
    "composite": true,
    "paths": {
      "autogenerated/*": ["./autogenerated/*"],
      "ui/*": ["./ui/*"],
      "app/*": ["./app/*"]
    }
  }
}
